package cz.cuni.mff.d3s.jdeeco.ensembles.intelligent.z3

data contract Rescuer
	pos : int
	isLeader : bool
	trainId : int
end

data contract FireFighter
	trainId : int
end

ensemble PendolinoEnsemble
	id trainId : int
	membership
		roles
			leader : Rescuer
			rescuers [2..4] : Rescuer
			firefighter [0..1] : FireFighter		
		
		constraints
		constraint leader.pos < 0
		
		fitness 0
		
	knowledge exchange
		x := x
end