package cz.cuni.mff.d3s.jdeeco.ensembles.intelligent.z3

data contract Rescuer
	pos : int
	isLeader : bool
	trainId : int
end

data contract FireFighter
	trainId : int
end

ensemble PendolinoEnsemble
	id trainId : int
	membership
		roles
			rescuers [1..2] : Rescuer	
		
		constraints
		constraint rescuers.pos != rescuers.pos
		
		fitness 0
		
	knowledge exchange
		x := x
end